{
    "name": "webkul/payslip",
    "description": "Employee Payslip Management plugin for AureusERP",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Webkul",
            "email": "support@webkul.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0",
        "filament/filament": "^3.0",
        "webkul/employees": "*",
        "webkul/accounts": "*",
        "barryvdh/laravel-dompdf": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Webkul\\Payslip\\": "src/",
            "Webkul\\Payslip\\Database\\Factories\\": "database/factories/",
            "Webkul\\Payslip\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Webkul\\Payslip\\PayslipServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}